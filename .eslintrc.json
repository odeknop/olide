{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "parser": "babel-eslint",
  "extends": "aribnb",
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}